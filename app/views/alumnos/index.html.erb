<div class="row">
	<div class="ten centered columns">
	<h1>Alumnos</h1>
	<table>
		<tr>
			<th>Nc</th>
			<th>Nombre</th>
			<th>Email</th>
			<th>Password</th>
			<th>Avance</th>
			<th></th>
			<th></th>
			<th></th>
		</tr>
		<% @alumnos.each do |alumno| %>
		<tr>
			<td><%= alumno.nc %></td>
			<td><%= alumno.nombre %></td>
			<td><%= alumno.email %></td>
			<td><%= alumno.avance %></td>
			<td><%= link_to 'Show', alumno %></td>
			<td><%= link_to 'Edit', edit_alumno_path(alumno) %></td>
			<td><%= link_to 'Destroy', alumno, confirm: 'Are you sure?', method: :delete %></td>
		</tr>
		<% end %>
	</table>
	<br />
	<%= link_to 'New Alumno', new_alumno_path,:class=>"button success" %>
	</div>
</div>
<br /> <br /><br /><br />